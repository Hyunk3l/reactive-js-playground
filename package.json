{
  "name": "reactive-js-playground",
  "version": "1.0.0",
  "description": "Playground for Reactive JS (RxJS)",
  "repository": "https://github.com/Hyunk3l/reactive-js-playground",
  "main": "app.js",
  "dependencies": {
    "jquery": "^2.2.1",
    "rx": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "node-sass": "^3.4.2",
    "opener": "^1.4.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "scripts": {
    "dist": "npm run dist:scripts && npm run dist:styles",
    "dist:scripts": "babel src --out-dir dist",
    "dist:styles": "node-sass src/index.scss dist/index.scss",
    "dev": "npm run dev:open && npm run dev:serve",
    "dev:serve": "webpack-dev-server --devtool eval --progress --colors --content-base .",
    "dev:open": "opener http://localhost:8080"
  },
  "author": "Fabrizio Di Napoli <hyunk3l@gmail.com>",
  "license": "ISC"
}
